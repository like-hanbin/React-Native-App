{
  "expo": {
    "name": "TestApp",
    "slug": "myTodoApp",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FCCF4D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "me.hanbin.todo",
      "icon": "./assets/icon.png",
      "supportsTablet": true
    },
    "android": {
      "package": "me.hanbin.todo",
      "icon":"./assets/icon-logo.png",
      "adaptiveIcon": {
        "backgroundColor" : "#6830CF",
        "foregroundImage": "./assets/splash.png"
      }
    }
  }
}